<?xml version="1.0" encoding="utf-8" ?>
<level>
  <!--All entities have coordinates and velocity. If there is no velocity tag, velocity will default to zero. See mario for example-->
  <!--Multiple of the same entity can be added-->
  
  <!--100x100 is minimum to avoid breaking-->
  <levelwidth>800</levelwidth>
  <levelheight>480</levelheight>
  <player>
    <state>standard</state>
    <coordinates>
      <x>300</x>
      <y>300</y>
    </coordinates>
    <velocity>
      <x>0</x>
      <y>0</y>
    </velocity>
  </player>

  <coin>
    <coordinates>
      <x>180</x>
      <y>100</y>
    </coordinates>
    <!--visibility has values visible, brick, and question. not yet implemented since we don't have to worry about block responses this sprint.-->
    <visibility>visible</visibility>
  </coin>

