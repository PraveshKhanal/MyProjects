<?xml version="1.0" encoding="utf-8" ?>
<level>
  <!--All entities have coordinates and velocity. If there is no velocity tag, velocity will default to zero. See mario for example-->
  <!--Multiple of the same entity can be added-->
  
  <!--100x100 is minimum to avoid breaking-->
  <!--Requires at least 468 by 468 for it to work-->
  <levelwidth>468</levelwidth>
  <levelheight>468</levelheight>

  <!--Default position for Mario: X = 60, Y = 320-->
  <player>
    <state>standard</state>
    <coordinates>
      <x>60</x>
      <y>300</y>
    </coordinates>
    <velocity>
      <x>0</x>
      <y>0</y>
    </velocity>
  </player>


  <!--Vertical left bricks-->
  <brickcolored>
    <coordinates>
      <x>26</x>
      <y>130</y>
    </coordinates>
    <stack>
      <x>1</x>
      <y>10</y>
    </stack>
  </brickcolored>


  <!--Ground Floor-->

  <groundcolored>
    <coordinates>
      <x>26</x>
      <y>390</y>
    </coordinates>
    <stack>
      <x>16</x>
      <y>1</y>
    </stack>
  </groundcolored>
  
  <groundcolored>
    <coordinates>
      <x>26</x>
      <y>416</y>
    </coordinates>
    <stack>
      <x>16</x>
      <y>1</y>
    </stack>
  </groundcolored>

  <!--Middle Floating bricks-->
  <brickcolored>
    <coordinates>
      <x>130</x>
      <y>130</y>
    </coordinates>
    <stack>
      <x>7</x>
      <y>1</y>
    </stack>
  </brickcolored>

  <!--Middle horizontal bricks-->
  <brickcolored>
    <coordinates>
      <x>130</x>
      <y>312</y>
    </coordinates>
    <stack>
      <x>7</x>
      <y>1</y>
    </stack>
  </brickcolored>
  
  <brickcolored>
    <coordinates>
      <x>130</x>
      <y>338</y>
    </coordinates>
    <stack>
      <x>7</x>
      <y>1</y>
    </stack>
  </brickcolored>

  <brickcolored>
    <coordinates>
      <x>130</x>
      <y>364</y>
    </coordinates>
    <stack>
      <x>7</x>
      <y>1</y>
    </stack>
  </brickcolored>
  
  
  <!--End of level-->
</level>